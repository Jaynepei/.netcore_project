        <div class="row">
            <div class="col-sm-4">
                Item
            </div>
            <div class="col-sm-2">
                Activation Code
            </div>
            <div class="col-sm-2 text-center">
                Unit Price
            </div>
            @*<div class="col-sm-2 text-center">
                Activation Code
            </div>*@
            @*<div class="col-sm-2 text-center">
